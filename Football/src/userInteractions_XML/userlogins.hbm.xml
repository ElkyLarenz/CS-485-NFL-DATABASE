<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mpping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Hibernate mapping file, it maps a Java class creditCard(ID,cradN) to database table  -->

<hibernate-mapping>
	<class name="Lab6.userLoginInfo" table="userlogins">  <!-- Java class userLogins to table userLogins --> 
		<id name="ID" type="int">
			<generator class="increment" /> <!-- ID is automatically generated -->
		</id>
		<property name="cardN" column="userName" type="string" />
		<property name="passW" column="Password" type="string" />
		<property name="FN" column="FN" type="string" />
		<property name="LN" column="LN" type="string" />
		
	</class>
</hibernate-mapping>